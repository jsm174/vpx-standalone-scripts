Sub FlashFadeLevel(Level,TargetLevel,TimerEnabled)
	TimerEnabled = True
	if round(TargetLevel,1) > round(Level,1) Then
		Level = Level + 0.3
		if Level > 1 then Level = 1
	Elseif round(TargetLevel,1) < round(Level,1) Then
		Level = Level * 0.85 - 0.01
		if Level < 0 then Level = 0
	Else
		Level = round(TargetLevel,1)
		TimerEnabled = False
	end if
End Sub

