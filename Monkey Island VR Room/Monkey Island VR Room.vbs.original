		Exit Sub
	End If
	If LeftOrbitStart = 2 Then
		LeftOrbitStart = 0
		Exit Sub
	End If
	If RightOrbitStart = 0 Then
		DMD "_", CL(1, "RIGHT LANE"), "_", eNone, eScrollLeft, eNone, 1, False, ""
		DMD "_", CL(1, "RIGHT LANE"), "_", eNone, eNone, eNone, 1, False, ""
