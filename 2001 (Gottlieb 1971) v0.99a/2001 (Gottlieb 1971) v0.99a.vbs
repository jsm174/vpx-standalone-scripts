	Bumper1Light.state=0
	Bumper2Light.state=0
	KickLight1.state=0
	KickLight2.state=0
	'KickLight3.state=0
	KickLight4.state=0
	KickLight5.state=0
	AdvanceLightCounter=0
	Players=0
	RotatorTemp=1
	InProgress=false
	TargetLightsOn=false
	EMReel6.SetValue(0)
	EMReel7.SetValue(0)
	EMReel8.SetValue(0)
	EMReel9.SetValue(0)
	ScoreText.text=HighScore

	If B2SOn Then
		if Match=0 then
			Controller.B2SSetMatch 10
		else
			Controller.B2SSetMatch Match
		end if
		Controller.B2SSetScoreRolloverPlayer1 0
		Controller.B2SSetScoreRolloverPlayer2 0
		Controller.B2SSetScoreRolloverPlayer3 0
		Controller.B2SSetScoreRolloverPlayer4 0
		Controller.B2SSetScore 3,HighScore
		Controller.B2SSetTilt 1
		Controller.B2SSetCredits Credits
		Controller.B2SSetGameOver 1
	End If
	for i=1 to 4       'ReplayTable
    player=i
		If B2SOn Then 
			Controller.B2SSetScorePlayer player, 0
	End If
	next    'ReplayTable          
	bump1=1
	bump2=1
	InitPauser5.enabled=true
	If Credits > 0 Then DOF 118, DOFOn
	BallInPlay=0	'ReplayTable
