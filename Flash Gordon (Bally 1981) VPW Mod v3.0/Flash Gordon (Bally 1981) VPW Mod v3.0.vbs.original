Sub AnimateStar(group, sw, action) ' Action = 1 - to drop, 0 to raise
	Dim BP
	If action = 1 Then
		RandomSoundRollover
		For Each BP in group : BP.transz = -6 : Next
	Else
		For Each BP in group
			BP.transz = BP.transz + 0.5 
			if BP.transz = -3 and Rnd() < 0.05 Then
				sw.timerenabled = 0
			Elseif BP.transz >= 0 Then
				BP.transz = 0
				sw.timerenabled = 0
			End If
		Next
	End If
