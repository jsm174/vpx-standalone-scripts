	If Controller.Lamp(45) = 0 Then: VR_BackglassGameOver.visible=0: 			Else: VR_BackglassGameOver.visible=1 			'Game Over
	If Controller.Lamp(61) = 0 Then: VR_BackglassTilt.visible=0: 				Else: VR_BackglassTilt.visible=1 				'Tilt
	
	
